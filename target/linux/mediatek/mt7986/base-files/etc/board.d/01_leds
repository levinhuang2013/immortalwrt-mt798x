#!/bin/sh

. /lib/functions.sh
. /lib/functions/leds.sh
. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case $board in
xiaomi,redmi-router-ax6000*)
	ucidef_set_led_default "status" "status" "rgb:status" "1"
	ucidef_set_led_netdev "wan" "wan" "rgb:network" "eth1"
	;;
zyxel,ex5700)
	ucidef_set_led_netdev "wlan2g" "WLAN2G" "amber:net" "rai0" "tx rx"
	ucidef_set_led_netdev "wlan5g" "WLAN5G" "green:net" "rax0" "tx rx"
	ucidef_set_led_netdev "wlan6g" "WLAN6G" "red:net" "raix0" "tx rx"
	;;
esac

board_config_flush

exit 0
